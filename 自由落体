public class aixin {
    public static void main(String[] args) {
        int length=100;
        int sum=0;
        for(int i=0;i<10;i++){
            sum+=length;
            System.out.format("第%d次总长"+sum+";"+"第%d次高度"+length,i+1,i+1);;
            System.out.println();
            length=length >> 1;

        }

    }
}
