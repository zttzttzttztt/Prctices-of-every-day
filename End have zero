import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int cout = cal(n);
        System.out.println(cout);
    }
    private static int cal(int n) {
        if(n < 5)
            return 0;
        else {
            n = n / 5;
            return n + cal(n);
        }
    }
}
