import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        
        Scanner input = new Scanner(System.in);
        int num = input.nextInt();
        String t = "";
 
        while (num != 0) {
            t += num % 10;
            num /= 10;
        }
        System.out.println(t);
    }
}
